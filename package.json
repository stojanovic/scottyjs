    "get-folder-size": "^1.0.0",
    "inquirer": "^3.2.2",
    "mime": "^1.4.1",
    "minimist": "^1.2.0",
    "snyk": "^1.317.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-defaults": "^9.0.0",
    "jasmine": "^2.7.0",
    "jasmine-spec-reporter": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stojanovic/scottyjs.git"
  },
  "bugs": {
